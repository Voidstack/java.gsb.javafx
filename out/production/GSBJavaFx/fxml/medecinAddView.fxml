<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootPane" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.gsb.controller.ControllerMedecinAdd">
    <children>
        <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <VBox alignment="CENTER">
                    <children>
                  <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity">
                     <children>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Code Medecin : " />
                              <TextField fx:id="tfCM" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nom : " />
                              <TextField fx:id="tfNom" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Prenom : " />
                              <TextField fx:id="tfPre" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Adresse : " />
                              <TextField fx:id="tfAdr" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Ville : " />
                              <TextField fx:id="tfVille" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Code Postal : " />
                              <TextField fx:id="tfCP" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Téléphone : " />
                              <TextField fx:id="tfTel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Potentiel : " />
                              <TextField fx:id="tfPot" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Spécialité : " />
                              <TextField fx:id="tfSpe" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="200.0" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
                  <Text fx:id="txtResult" strokeType="OUTSIDE" strokeWidth="0.0" />
                        <HBox alignment="CENTER">
                            <children>
                                <Button fx:id="btAdd" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#onClick" prefHeight="35.0" prefWidth="100.0" text="Ajouter" textFill="WHITE" />
                            </children>
                            <padding>
                                <Insets top="10.0" />
                            </padding>
                        </HBox>
                    </children>
                </VBox>
            </children>
        </StackPane>
    </children>
</AnchorPane>
