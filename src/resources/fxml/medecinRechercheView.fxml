<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootPane" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.gsb.controller.ControllerRecherche">
    <children>
        <StackPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER">
               <children>
                  <TextField fx:id="tfSearch" prefWidth="250.0" />
                  <Button fx:id="btSearch" mnemonicParsing="false" onMouseClicked="#onClick" text="Recherche" />
                  <Text fx:id="txtResult" strokeType="OUTSIDE" strokeWidth="0.0" />
               </children>
            </HBox>
         </children>
        </StackPane>
    </children>
</AnchorPane>
